{
  ".pubnub.yml": [
    { "pattern": "\\s+package-name: php-(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?).zip$", "cleared": false },
    { "pattern": "pubnub/php/releases/tag/(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?)$", "cleared": false }
  ],
  "README.md": [
    { "pattern": "^\\s{2,}\"pubnub\/pubnub\": \"(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?)\"$", "cleared": false }
  ],
  "composer.json": [
    { "pattern": "^\\s{2,}\"version\": \"(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?)\",$", "cleared": false }
  ],
  "src/PubNub/PubNub.php": [
    { "pattern": "^\\s{2,}const SDK_VERSION = \"(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?)\";$", "cleared": false }
  ]
}
